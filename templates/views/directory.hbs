            <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDDf23BdJHCWYsNv3Iin43vvQ87X9xflOk&callback=initMap" async defer></script>
            <script type="text/javascript" src="js/directory-lib/directory-script.js"></script>
            {{!-- <script type="text/javascript" data-main="js/directory-app" src="js/directory-lib/require.js"></script> --}}
        </main>
        {{>footer}}
    </body>
</html>

